

<div class="document">
  <div class="row">
    <div class="span12">
      <h2><%= t('frda.collection.member_detail') %></h2>
    </div>
  </div>

  <div class="row document-details">
		<% if @document.images_item? %>
			<%= render :partial=>'show_images_item' %>
		<% elsif @document.ap_item? %>
			<%= render :partial=>'show_ap_item' %>
		<% else %>
			<%= render :partial=>'show_collection_item' %>
		<% end %>
  </div>

</div>
