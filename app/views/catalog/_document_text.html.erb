<%# This is an AP results view %>

<div id="documents" class="text">
  <%- documents.each_with_index do |document, index| -%>
    <div class="row">
      <div class="span9">
        <%= render :partial => "document_header", :locals => {:document_counter => index, :document => document} %>
      </div>
    </div>
  <%- end -%>
</div>
