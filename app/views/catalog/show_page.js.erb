<%if @document%>
	$('#item_detail').html("<%= escape_javascript(render( :partial => "show_ap_item")).html_safe %>");
	clearMessages();
	showOnLoad();
	showImageViewer('<%=@document.first_image(:size=>:full,:format=>"none")%>','ap_page_image');
<% else %>
	showMessage('<%=t('frda.show.not_found')%>','alert');
<%end%>

