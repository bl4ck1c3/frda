<div class="row">
  <div class="span12">
    <% if  @document.collection? %>
      <%= raw truncate(render_document_heading(:h2).html_safe,:length=>200) %>
    <% else %>
      <h2><%= t('frda.collection.member_detail') %></h2>
    <% end %>
  </div>
</div>

<div class="row document-details">
  <div class="span6">
    AP Item
  </div>

  <div class="span6 metadata">
    <%= render_document_heading(:h4).html_safe %>

  AP Metadata
  </div>
</div>
